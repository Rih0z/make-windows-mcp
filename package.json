{
  "name": "windows-mcp-setup",
  "version": "1.0.0",
  "description": "Windows VM MCP Server setup for Claude Code",
  "scripts": {
    "setup": "npm install",
    "configure": "node scripts/configure.js",
    "start": "node server.js",
    "dev": "NODE_ENV=development node server.js",
    "test": "jest",
    "test:security": "jest --testNamePattern=\"Security\"",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "production:check": "node scripts/production-setup.js check",
    "production:generate": "node scripts/production-setup.js generate",
    "lint": "echo \"No linting configured\" && exit 0"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "ssh2": "^1.15.0",
    "ping": "^0.4.4",
    "helmet": "^7.1.0"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "jest-environment-node": "^29.7.0"
  },
  "keywords": [
    "mcp",
    "claude-code",
    "windows",
    "build-server"
  ],
